<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Conteudo 3"
             NavigationPage.HasBackButton="False"
             x:Class="AppGallery.XamarinForms.Paginas.PaginaDeNavegacao.Page3">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Inserir Pagina na Pilha" Clicked="InserirPaginaPilha" />
            <Button Text="Remover Pagina da Pilha" Clicked="RemoverPaginaPilha" />
            
            <Label Text="Pagina de Conteudo 3!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Voltar" Clicked="VoltarParaPaginaAnterior" />
            <Button Text="Voltar Para Inicio" Clicked="VoltarParaInicio" />
            
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>